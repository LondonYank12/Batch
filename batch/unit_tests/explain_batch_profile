export CLASSPATH=${CLASSPATH}:${CLASSES_DIR}/xalan.jar
export CLASSPATH=${CLASSPATH}:${CLASSES_DIR}/xerces.jar
export CLASSPATH=${CLASSPATH}:${CLASSES_DIR}/xml-apis.jar
export CLASSPATH=${CLASSPATH}:${CLASSES_DIR}/xml4j.jar

#
# Log4J
#
#--------------------------------------------------------------------------------------
export CLASSPATH=${CLASSPATH}:/sbcimp/run/pd/jakarta-log4j/1.1.3/dist/lib/log4j.jar 
#
# PERL
#
#--------------------------------------------------------------------------------------
export PERL=/sbcimp/run/pd/perl/5.8.8/bin/perl

PERL5LIB=/sbcimp/run/pd/cpan/5.8.8-2006.03/lib:$EXPLAIN_HOME/perlib
PERL5LIB=$PERL5LIB:/sbcimp/run/pkgs/CORE/12.1.0/lib
export PERL5LIB

export EXPLAIN_BATCH_TMP=/tmp/

#
# MICROMUSE_ENABLED is either TRUE or FALSE.
# Determins if logs are written to Micromuse or not
# Explain has been accused of spamming Micromuse as every Log message is written to them.
# Needs review before if can be reenabled
#--------------------------------------------------------------------------------------
export MICROMUSE_ENABLED=FALSE
#export MICROMUSE_CONF=$EXPLAIN_HOME/conf/micromuse/prod_log.conf
export MICROMUSE_CONF=$EXPLAIN_HOME/conf/micromuse/dev_log.conf

#--------------------------------------------------------------------------------------
# ORACLE
#--------------------------------------------------------------------------------------

export ORACLE_HOME=/sbcimp/run/tp/oracle/client/v10.1.0.4.0-32bit
export CLASSPATH=${CLASSPATH}:$EXPLAIN_HOME/classes/oracle/classes12.jar

export PATH=${PATH}:${ORACLE_HOME}/bin
export LD_LIBRARY_PATH=${LD_LIBRARY_PATH}:${ORACLE_HOME}/lib

# GPAS Extract
export EXPLAIN_BATCH_ORACLE_GPAS_EXTRACT_SID=OPGCP3
export EXPLAIN_BATCH_ORACLE_GPAS_EXTRACT_URL=jdbc:oracle:thin:@op1p1447ora.opf.swissbank.com:1524:OPGCP3
