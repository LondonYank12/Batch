then
	. /sbcimp/shared/config/CA/Autosys/v4.0/devl/kshDLN
fi

# Production
#if [ -f /sbcimp/shared/config/CA/Autosys/v4.0/prod/autouser/kshellPSM ]
#then
#	. /sbcimp/shared/config/CA/Autosys/v4.0/prod/autouser/kshellPSM
#fi

# PASSWORDS 
# You will need to match up the passwords here with the associated DB 
# or account specified in the main section of the profile

export EXPLAIN_BATCH_ORACLE_GPAS_EXTRACT_PWD=QWEQWE2
export EXPLAIN_BATCH_ORACLE_PWD=XWHWL85
export EXPLAIN_BATCH_WMUSA_FTP_PWD=GUEST

export HOST="$(hostname)"

# EXPLAIN_HOME needs to be set to the root of the batch directory
# if you are currently developing code this should be set to the path of your ClearCase vob on UNIX
# which is most likely /vobs/ZUR_GCOMP_EXPLAIN/batch
# In Production, this should be set to /sbcimp/run/pkgs/gc_explain/prod
# In non Production environments, this should be set to /sbcimp/run/pkgs/gc_explain/[version number]
#
#export EXPLAIN_HOME=/sbcimp/run/pkgs/gc_explain/v1_34
export EXPLAIN_HOME=/vobs/ZUR_GCOMP_EXPLAIN/batch

# Add the Explain bin and sbin directories to the path.  This will allow Explain commands
# to be executed from any directory
export PATH=${PATH}:${EXPLAIN_HOME}/bin:${EXPLAIN_HOME}/sbin

# EXPLAIN_LOG is the root of the Explain log directory
# If you are developing code this can be wherever you like, probably somewhere in your home directory
# should be set to /sbcimp/dyn/logfiles/gcb/explain/prd
#
export EXPLAIN_LOG=$HOME/temp/explain_log
#export EXPLAIN_LOG=/sbcimp/dyn/logfiles/gcb/explain/dev
#export EXPLAIN_LOG=/sbcimp/dyn/logfiles/gcb/explain/bis
#export EXPLAIN_LOG=/sbcimp/dyn/logfiles/gcb/explain/uat
#export EXPLAIN_LOG=/sbcimp/dyn/logfiles/gcb/explain/prd

# EXPLAIN_DATA root of the Explain data directory
# If you are developing code this can be wherever you like, probably somewhere in your home directory
# In Production this should be set to /sbcimp/dyn/data/gcb/explain/prd
#
export EXPLAIN_DATA=$HOME/temp/explain_data
#export EXPLAIN_DATA=/sbcimp/dyn/data/gcb/explain/dev
#export EXPLAIN_DATA=/sbcimp/dyn/data/gcb/explain/bis
#export EXPLAIN_DATA=/sbcimp/dyn/data/gcb/explain/uat
#export EXPLAIN_DATA=/sbcimp/dyn/data/gcb/explain/prd

# EXPLAIN_BATCH_NOTIFICATION_EMAIL defines where the Explain Batch 'Error' email gets sent
#
export EXPLAIN_BATCH_NOTIFICATION_EMAIL=owen.singleton@ubs.com

# EXPLAIN_STATUS_EMAIL defines where the Explain Batch 'Status / Blue' email gets sent
# TODO change to this env var as bus_stat_summary.pl has the address hard coded my @to_address = ("DL-EXPLAIN-STATUS\@ubs.com");
export EXPLAIN_STATUS_EMAIL=owen.singleton@ubs.com

# EXPLAIN_BATCH_DEADLINE is the time at which the polling scripts give up and fail
export EXPLAIN_BATCH_DEADLINE=16:00

# VPD_STATUS email defines to recipents of the daily VPD status check
# covers both GPAS and Explain databases
export VPD_STATUS_EMAIL=owen.singleton@ubs.com

# EXPLAIN_BATCH_ERROR_LEVEL determins which messages are actually written to the logs
# Does not appear to be consistantly applied, so best not to change this value
export EXPLAIN_BATCH_ERROR_LEVEL=20

# EXPLAIN_BATCH_DISK_SPACE_ALERT_LIMIT determines min percentage of available space on $EXPLAIN_DATA 
# before the check_explain.pl script fails 
export EXPLAIN_BATCH_DISK_SPACE_ALERT_LIMIT=80

#
# GPG
#
export PATH=${PATH}:/home/gcomp/local/gnupg-1.0.7/bin
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/sbcimp/run/pd/gcc/3.1/lib

#
# Java
#
#--------------------------------------------------------------------------------------
export JAVA_HOME=/sbcimp/run/tp/sun/jre/v1.5.0_06
export CLASSPATH=.:$EXPLAIN_HOME/classes
export PATH=$JAVA_HOME/bin:$PATH
export JVM_PARAM=-Xmx320M
export EXPLAIN_BATCH_CLASS_PATH=$CLASSPATH

#
# OPEN-ADAPTOR
# Class path information for OA
#
#--------------------------------------------------------------------------------------
CLASSES_DIR=$EXPLAIN_HOME/3rdparty/openadaptor/1_4_0/classes
