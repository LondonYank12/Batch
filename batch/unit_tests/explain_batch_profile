# Explain Batch
export ORACLE_SID=OPGCEXD1.world
export EXPLAIN_BATCH_ORACLE_SID=$ORACLE_SID
export EXPLAIN_BATCH_ORACLE_URL=jdbc:oracle:thin:@op1p1447ora.opf.swissbank.com:1522:OPGCEXD1
